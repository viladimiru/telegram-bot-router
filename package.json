{
  "name": "telegram-bot-router",
  "version": "1.0.0",
  "description": "Telegram bot router",
  "keywords": [
    "tg",
    "telegram",
    "node",
    "telegram-bot"
  ],
  "type": "module",
  "main": "index.js",
  "scripts": {
    "eslint-fix": "eslint . --fix",
    "prettier-fix": "prettier . --write",
    "prepare": "husky && husky install",
    "start": "node --watch --loader ts-node/esm bot.ts"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node-telegram-bot-api": "^0.64.7",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "node-telegram-bot-api": "^0.66.0"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
